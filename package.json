{
  "name": "multi-agent-temporal",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "orchestrator": "ts-node orchestrator/worker.ts",
    "app-designer": "ts-node agents/app-designer/worker.ts",
    "terraform-agent": "ts-node agents/terraform/worker.ts",
    "security-agent": "ts-node agents/security-checker/worker.ts",
    "cost-agent": "ts-node agents/cost-estimator/worker.ts",
    "review-agent": "ts-node agents/manual-review/worker.ts",
    "start:runner": "ts-node runner.ts",
    "test:agent": "ts-node scripts/generate-agent.ts summarizer --prompt 'Summarize the following: {{input}}' --systemRole 'You are a text summarizer' --createdBy 'francisco'",
    "dev:all": "concurrently -n orchestrator,app,terraform,security,cost,review -c cyan,blue,green,yellow,magenta,gray \"npm:orchestrator\" \"npm:app-designer\" \"npm:terraform-agent\" \"npm:security-agent\" \"npm:cost-agent\" \"npm:review-agent\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@temporalio/client": "^1.11.7",
    "@temporalio/common": "^1.11.7",
    "@temporalio/worker": "^1.11.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "handlebars": "^4.7.8",
    "mongodb": "^6.15.0",
    "openai": "^4.93.0",
    "uuid": "^11.1.0",
    "yaml": "^2.7.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
